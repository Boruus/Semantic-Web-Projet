        sh:minCount 0 ;
    ] ;
    sh:property [
        sh:path schema:alternateName;  
        sh:datatype xsd:string ;
        sh:minCount 0 ;
    ] ;
    sh:property [
        sh:path bulbapedia:jtranslit; 
        sh:datatype xsd:string ;
        sh:minCount 0 ;
    ] ;
    sh:property [
        sh:path bulbapedia:generation;  
        sh:datatype xsd:string ;
        sh:minCount 0 ;
    ] ;
    sh:property [
        sh:path bulbapedia:expyield;  
        sh:datatype xsd:string ;
        sh:minCount 0 ;
    ] ;
    sh:property [
        sh:path bulbapedia:evsa;  
        sh:datatype xsd:string ;
