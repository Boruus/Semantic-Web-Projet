        sh:minCount 0 ;
    ] ;
    sh:property [
        sh:path bulbapedia:height-ftin;  
        sh:datatype xsd:string ;
        sh:minCount 0 ;
    ] ;
    sh:property [
        sh:path bulbapedia:lv100exp;  
        sh:datatype xsd:string ;
        sh:minCount 0 ;
    ] ;
    sh:property [
        sh:path bulbapedia:tmname;  
        sh:datatype xsd:string ;
